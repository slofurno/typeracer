{
  "consul": "localhost:8500",
  "stopTimeout": -1,
  "logging": {
    "level": "INFO",
    "format": "default",
    "output": "stdout"
  },
  "preStop": "echo \"peace among worlds\"",
  "services": [
    {
      "name": "typeracer-{{ .PORT0 }}",
      "port": {{ .PORT0 }},
      "health": "/usr/bin/curl --fail -s http://localhost:{{ .PORT0 }}/api",
      "poll": 3,
      "ttl": 10
    }
  ],
  "backends": [
  ]
}
